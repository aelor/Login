<div id="content">
	
				<script type="text/javascript">
		           $(document).ready(function() {
				   $('#emailid').focus();
				       $("#loginform").validationEngine('attach');
					});
					
			    </script>
		
			
			<%= form_tag sessions_path,:id=>"loginform" do %>
			<h2>Login</h2>
			  <p>
			    <%= label_tag :email %><br />
			    <%= text_field_tag :email, params[:email], :id =>"emailid", :class=>"validate[required,custom[email]]"%>
			  </p>
			  <p>
			    <%= label_tag :password %><br />
			    <%= password_field_tag :password %>
			  </p>
			  <br />
			  <p>
			  <%= submit_tag "Log in", :class=>"button" %>
			  </p>
			<% end %>

</div> 
